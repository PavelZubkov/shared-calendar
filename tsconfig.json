{
  "extends": "@tsconfig/node10/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "node",
    "typeRoots": ["@types", "node_modules/@types"],
    "baseUrl": ".",
    "paths": {
      "@architect/shared/*": ["src/shared/*"]
    }
  },
  "exclude": ["node_modules", "@types"]
}
